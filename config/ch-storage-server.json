{
    "console": true,
    "daemon": false,
    "run-for": 600000,
    "run-forever": false,
    "server": {
        "port": 8888
    },
    "storage": {
        "root": "./data"
    },
    "streams": [
        {
            "name": "living-room-1",
            "path": "/security/camera/media/living-1/*"
        },
        {
            "name": "room-1",
            "path": "/security/camera/media/room-1/*"
        }
    ],
    "purge": {
        "ttl-s": 60,
        "interval-s": 2
    }
}
